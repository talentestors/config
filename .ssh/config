# gitee
Host gitee.com
  HostName gitee.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa.gitee

# github
Host github.com
  HostName github.com
#  Port 443	    # Using SSH over the HTTPS port
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa.github

Host *
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa
